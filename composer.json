{
    "name": "gilbertogiro/laravel-aux",
    "type": "library",
    "description": "",
    "keywords": [
        "laravel-aux"
    ],
    "homepage": "https://github.com/GilbertoGiro/laravel-aux",
    "license": "MIT",
    "authors": [
        {
            "name": "Gilberto Giro Resende",
            "email": "gilberto.giro.resende@gmail.com",
            "homepage": "https://github.com/GilbertoGiro",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.0",
        "barryvdh/laravel-cors": "^0.11.0",
        "doctrine/dbal": "^2.7",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3"
    },
    "autoload": {
        "psr-4": {
            "GilbertoGiro\\laravel-aux\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GilbertoGiro\\laravel-aux\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}